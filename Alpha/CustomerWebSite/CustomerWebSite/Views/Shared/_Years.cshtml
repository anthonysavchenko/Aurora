@model CustomerWebSite.Models.YearsViewModel

@if (Model.Years.Length > 0)
{
    <div class="row">
        <div class="col-md-12 text-center">
            <ul class="pagination">
                @for (int i = 0; i < Model.Years.Length; i ++)
                {
                    <li @(Model.Years[i] == Model.CurrentYear ? "class=active" : "")>
                        @Html.ActionLink(Model.Years[i].ToString(), "PaymentsAndCharges", "Home", new { account = Model.Account, year = Model.Years[i] }, null)
                    </li>
                }
            </ul>
        </div>
    </div>
}